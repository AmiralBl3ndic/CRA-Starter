{
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@/*": [
				"src/*"
			],
			"@components/*": [
				"src/components/*"
			],
			"@pages/*": [
				"src/pages/*"
			],
			"@style/*": [
				"src/style/*"
			],
			"@images/*": [
				"src/assets/images/*"
			]
		}
	},
	"include": [
		"src/**/*"
	]
}